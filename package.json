{
  "name": "mi-cv",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node -e \"process.env.NODE_ENV='production'; require('child_process').execSync('node src/index.js', {stdio: 'inherit'})\"",
    "dev": "node --watch src/index.js",
    "test": "node -e \"process.env.NODE_ENV='test'; require('child_process').execSync('jest', {stdio: 'inherit'})\"",
    "test:unit": "node -e \"process.env.NODE_ENV='test'; require('child_process').execSync('node --experimental-print-required-tla ./node_modules/jest/bin/jest.js test/unit', {stdio: 'inherit'})\"",
    "test:integration": "node -e \"process.env.NODE_ENV='test'; process.env.RUN_GLOBAL_SETUP='true'; require('child_process').execSync('jest test/integration --runInBand --detectOpenHandles', {stdio: 'inherit'})\"",
    "test:single": "node -e \"process.env.NODE_ENV='test'; require('child_process').execSync('jest --runInBand --detectOpenHandles', {stdio: 'inherit'})\"",
    "route:list": "node scripts/listarRutas.mjs",
    "test:watch": "jest --watch",
    "seed": "node scripts/run-seeders.js",
    "migrate:seed": "node scripts/run-migrate-seeders.js",
    "seed:test": "node -e \"process.env.NODE_ENV='test'; require('child_process').execSync('node scripts/run-seeders.js', {stdio: 'inherit'})\"",
    "rutas": "node scripts/listarRutas.mjs",
    "test:unit:debug": "node -e \"process.env.NODE_ENV='test'; require('child_process').execSync('node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand', {stdio: 'inherit'})\"",
    "test:coverage": "node -e \"process.env.NODE_ENV='test'; process.env.RUN_GLOBAL_SETUP='true'; require('child_process').execSync('jest --coverage --runInBand', {stdio: 'inherit'})\"",
    "sonar": "npm run test:coverage && sonar-scanner"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-list-endpoints": "^7.1.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.14.1",
    "sequelize": "^6.37.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@ljharb/tsconfig": "^0.3.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "sequelize-cli": "^6.6.3",
    "sonar-scanner": "^3.1.0",
    "supertest": "^7.1.1"
  }
}
